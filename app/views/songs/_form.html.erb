<%= form_for @song do |f| %>
<%= f.label "Title of Song: " %>
<%= f.text_field :title %>
<br>
<br>
<%= f.label "Name of Artist: " %>
<%= f.text_field :artist_name %>
<br>
<br>
<%= f.label "Genre: " %>
<%= f.text_field :genre %>
<br>
<br>
<%= f.label "Released: " %>
<%= f.check_box :released %>
<br>
<br>
<%= f.label "Year Released: " %>
<%= f.number_field :release_year %>
<br>

<%= f.submit %>
<% end %>

